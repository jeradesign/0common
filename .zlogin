# Executed *after* .zshrc for all *login* shells
# Stuff for all *interactive* shells should go in .zshrc

# Copy init files to my git repo dir
# (You probably don't need this)
cp ~/.bash_profile ~/proj/0common
cp ~/.emacs ~/proj/0common
cp ~/.zshenv ~/proj/0common
cp ~/.zprofile ~/proj/0common
cp ~/.zshrc ~/proj/0common
cp ~/.zlogin ~/proj/0common
